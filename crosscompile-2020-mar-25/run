mkdir build && cd build
cmake ../../hdf5 -DCMAKE_INSTALL_PREFIX=/tmp/hdf5/ -DCMAKE_TOOLCHAIN_FILE=../aarch64.cmake   \
    -DHDF5_BUILD_CPP_LIB=OFF -DONLY_SHARED_LIBS=ON -DHDF5_BUILD_HL_LIB=ON                  \
    -DHDF5_ENABLE_Z_LIB_SUPPORT=ON -DHDF5_ENABLE_SZIP_SUPPORT=OFF -DHDF5_ENABLE_SZIP_ENCODING=OFF -DBUILD_TESTING=OFF

